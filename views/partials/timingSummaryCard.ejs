<div class="card">
  <div class="card-block">
    <h4 class="card-title">Timing summary</h4>
    <table>
      <tr>
        <td>
          Time since last review
        </td>
        <td>
          <%= timingStats.timeSinceLastReview %>
        </td>
      </tr>
      <tr>
        <td>
          Average time between reviews
        </td>
        <td>
          <%= timingStats.averageTimeBetweenReviews %>
        </td>
      </tr>
      <tr>
        <td>
          Time since last android review
        </td>
        <td>
          <%= timingStats.timeSinceLastAndroidReview %>
        </td>
      </tr>
      <tr>
        <td>
          Average time between android reviews
        </td>
        <td>
          <%= timingStats.averageTimeBetweenAndroidReviews %>
        </td>
      </tr>
      <tr>
        <td>
          Time since last iOS review
        </td>
        <td>
          <%= timingStats.timeSinceLastIOSReview %>
        </td>
      </tr>
      <tr>
        <td>
          Average time between iOS reviews
        </td>
        <td>
          <%= timingStats.averageTimeBetweenIOSReviews %>
        </td>
      </tr>
    </table>
  </div>
</div>
